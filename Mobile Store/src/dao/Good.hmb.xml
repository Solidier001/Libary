<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dao.Good" table="Goods">
        <id name="id" column="Gid"><generator class="native"/></id>
        <property name="name" column="name" type="java.lang.String"/>
        <property name="discribe" column="discribe" type="java.lang.String"/>
        <property name="price" column="price" type="java.lang.String"/>
        <property name="src" column="src" type="java.lang.String"/>
    </class>
</hibernate-mapping>